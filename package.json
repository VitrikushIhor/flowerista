    "js-cookie": "^3.0.5",
    "lint-staged": "^15.2.2",
    "loki": "^0.35.0",
    "prettier": "^3.2.5",
    "storybook": "^8.1.0",
    "storybook-addon-mock": "^5.0.0",
    "stylelint": "^16.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.10",
    "vite-plugin-checker": "^0.6.4"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --list-different --write",
      "eslint \"**/*.{ts,tsx}\" --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "loki": {
    "chromeSelector": "body > *",
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7",
        "width": 375,
        "height": 667,
        "deviceScaleFactor": 2,
        "mobile": true
      }
    }
  }
}
